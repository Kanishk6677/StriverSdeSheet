void sort012(int *arr, int n)
{
int c0=0;
        int c1=0;
        int c2=0;
        for(int i=0;i<n;i++)
            {
                if(arr[i]==0) c0++;
            else if(arr[i]==1) c1++;
            else c2++;
            }
        int k;
        for(int i=0;i<c0;i++)
        {
            arr[k++]=0;
        }
        for(int i=0;i<c1;i++){ arr[k++]=1;}
        for(int i=0;i<c2;i++) {arr[k++]=2;}   
    }
